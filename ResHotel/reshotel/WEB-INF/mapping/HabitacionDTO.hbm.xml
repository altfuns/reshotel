<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="cr.ac.una.daw.reshotel.dto.HabitacionDTO" table="habitacion">
    <id name="id" column="id" type="int">
        <generator class="native"></generator>
    </id>
    <property name="hotelId" column="hotel" type="int"></property>
    <property name="ocupacionMaxima" column="ocupacion_maxima" type="int"></property>
    <property name="numero" column="numero" type="int"></property>
    <property name="mobiliario" column="mobiliario" type="string"></property>
    <property name="costo" column="costo" type="double"></property>
</class>
</hibernate-mapping>